---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="https://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>1.0</priority>
  </url>

  {% assign pages = site.html_pages | where_exp: "p", "p.url != '/404.html'" %}
  {% for p in pages %}
    {% unless p.url == '/' or p.path == 'index.html' %}
    <url>
      <loc>{{ site.url | append: site.baseurl | append: p.url | replace: '//', '/' }}</loc>
      <lastmod>{{ p.date | date_to_xmlschema | default: site.time | date_to_xmlschema }}</lastmod>
      <priority>0.8</priority>
    </url>
    {% endunless %}
  {% endfor %}
</urlset>
